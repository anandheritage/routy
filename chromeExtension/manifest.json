{
  "name": "V-Select",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Manage headers for all network requests",
  "content_security_policy": "default-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "V-Select",
    "default_popup": "popup.html"
  },
  "permissions":[
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
